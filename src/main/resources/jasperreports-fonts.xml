<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xsi:schemaLocation="http://www.springframework.org/schema/beans    http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">    <bean id="kanitFamily" class="net.sf.jasperreports.engine.fonts.SimpleFontFamily">        <property name="name" value="Kanit"/>        <property name="normal" value="fonts/Kanit-Regular.ttf"/>        <property name="bold" value="fonts/Kanit-Bold.ttf"/>        <property name="italic" value="fonts/Kanit-Italic.ttf" />        <property name="boldItalic" value="fonts/Kanit-BoldItalic.ttf" />        <property name="pdfEncoding" value="Identity-H"/>        <property name="pdfEmbedded" value="true"/>    </bean></beans>